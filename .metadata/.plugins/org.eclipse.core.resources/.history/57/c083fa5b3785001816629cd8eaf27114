package main.game;
import java.io.*;

public class game {
	
	int defaultHeight = 50;
	int defaultWidth = 50;
	int playerLocationX = 2;
	int playerLocationY = 1;
	Cell[][] tiles;
	Partition partition;

	public game() throws IOException {
		partition = new Partition(defaultHeight, defaultWidth);
		tiles = new Cell[defaultHeight][defaultWidth];
	}
	
	public game(int height, int width) throws IOException{ 
		defaultHeight = height;
		defaultWidth = width;
		partition = new Partition(defaultHeight, defaultWidth);
		tiles = new Cell[defaultHeight][defaultWidth];
	}
	
	public void moveNorth() {
		partition.moveNorth(playerLocationY, playerLocationX);
	}
	
	

}
